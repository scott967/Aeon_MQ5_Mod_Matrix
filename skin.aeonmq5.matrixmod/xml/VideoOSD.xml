				<posx>45</posx>
				<posy>110</posy>
				<width>1830</width>
				<height>38</height>
				<info>Player.ProgressCache</info>
				<midtexture>progress_mid50.png</midtexture>
				<visible>!VideoPlayer.Content(livetv)</visible>
			</control>
			<control type="progress">
				<posx>45</posx>
				<posy>110</posy>
				<width>1830</width>
				<height>38</height>
				<info>Player.Progress</info>
				<texturebg>progress_back50.png</texturebg>
