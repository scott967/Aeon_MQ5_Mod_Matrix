<window>
	<defaultcontrol always="true">100</defaultcontrol>
	<onunload>Skin.Reset(customosd)</onunload>
	<controls>
		<control type="group">
			<!-- Barra de info do modo custom -->
			<posy>0</posy>
			<include>Animation_TopPopup</include>
			<visible>Skin.HasSetting(customosd)</visible>
			<include>Dialogs_Bar</include>
			<control type="image">
				<!-- ativado azul -->
				<posx>75</posx>
				<posy>8</posy>
				<width>60</width>
				<height>28</height>
				<texture>grey.png</texture>
				<colordiffuse>FF007399</colordiffuse>
			</control>
			<control type="label">
				<posx>150</posx>
				<posy>6</posy>
				<width>375</width>
				<align>left</align>
				<font>Font_18A</font>
				<textcolor>FFFFFAF0</textcolor>
				<label>31426</label>
				<scroll>true</scroll>
				<scrollout>false</scrollout>
			</control>
			<control type="image">
				<!-- desativado vermelho -->
				<posx>375</posx>
				<posy>8</posy>
				<width>60</width>
				<height>28</height>
				<texture>grey.png</texture>
				<colordiffuse>FFB20000</colordiffuse>
			</control>
			<control type="label">
