				</control>
				<control type="label">
					<posx>1875</posx>
					<posy>195</posy>
					<width>450</width>
					<height>38</height>
					<align>right</align>
					<font>Font_18A</font>
					<textcolor>90FFFFFF</textcolor>
					<label>31311</label>
					<visible>!VideoPlayer.Content(LiveTV)</visible>
				</control>
				<control type="label">
					<posx>1875</posx>
					<posy>232</posy>
					<width>450</width>
					<height>38</height>
					<align>right</align>
					<font>Font_32</font>
					<textcolor>FFFFFAF0</textcolor>
					<label>$INFO[Player.FinishTime]</label>
					<visible>!VideoPlayer.Content(LiveTV)</visible>
				</control>
			</control>
			<control type="image">
				<posx>0</posx>
				<posy>172</posy>
				<height>135</height>
				<width>1920</width>
				<texture>gradient_small.png</texture>
			</control>
			<control type="image">
				<posx>0</posx>
				<posy>98</posy>
				<height>195</height>
				<width>1920</width>
				<colordiffuse>99FFFFFF</colordiffuse>
				<texture>osd_glass.png</texture>
			</control>
		</control>
		<include>BGBlack</include>
	</controls>
</window>
