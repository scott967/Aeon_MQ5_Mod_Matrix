			</control>
			<control type="slider" id="888">
				<posx>58</posx>
				<posy>112</posy>
				<width>1803</width>
				<height>32</height>
				<onup>Dialog.Close(2901)</onup>
				<ondown>100</ondown>
				<action>seek</action>
				<texturesliderbar>sliderback_large.png</texturesliderbar>
				<textureslidernib></textureslidernib>
				<textureslidernibfocus>progress_mid.png</textureslidernibfocus>
				<animation effect="fade" time="200">VisibleChange</animation>
			</control>
			<control type="label">
				<posx>135</posx>
				<posy>106</posy>
				<width>1650</width>
				<height>45</height>
				<align>center</align>
				<aligny>center</aligny>
				<font>Font_17A</font>
				<textcolor>FFFFFAF0</textcolor>
				<shadowcolor>grey</shadowcolor>
				<label>[B]$VAR[value_osd_timeduration][/B]</label>
			</control>
			<control type="label">
				<!-- icons label -->
				<posx>480</posx>
				<posy>249</posy>
				<width>960</width>
				<height>38</height>
				<align>center</align>
				<font>Font_18</font>
				<label>$VAR[value_osdvideo_title]</label>
				<textcolor>FFFFFAF0</textcolor>
			</control>
			<control type="group">
				<include>Animation_Fade</include>
				<visible>!Skin.HasSetting(customosd)</visible>
				<control type="label">
					<!-- hora atual -->
					<posx>45</posx>
					<posy>195</posy>
					<width>450</width>
					<height>38</height>
					<font>Font_18A</font>
					<textcolor>90FFFFFF</textcolor>
					<label>31310</label>
				</control>
				<control type="label">
					<posx>45</posx>
					<posy>232</posy>
					<width>450</width>
					<height>38</height>
					<font>Font_32</font>
					<textcolor>FFFFFAF0</textcolor>
